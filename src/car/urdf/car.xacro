    <origin xyz="0 0 0.002" rpy="0 0 0" />
  </xacro:VLP-16>

  <!-- 右前16线激光雷达 -->
  <xacro:include filename="VLP-16.urdf.xacro"/>
  <xacro:VLP-16 parent="plate_r" name="velodyne_right" topic="/velodyne_right" organize_cloud="${organize_cloud}" hz="10" samples="440" gpu="${gpu}">
